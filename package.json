{
  "name": "modelway",
  "version": "2.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Handy utility for Stable Diffusion",
  "author": "Al00X",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "lint": "npx eslint -c eslint.config.js \"src/**\"",
    "lint:fix": "npx eslint -c eslint.config.js \"src/**\" --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.38.0",
    "eslint-config-sheriff": "^11.7.0",
    "eslint-define-config": "^1.18.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-electron-plugin": "^0.8.2",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.1",
    "@mantine/hooks": "^6.0.1",
    "axios": "^1.3.4",
    "exifr": "^7.1.3",
    "jotai": "^2.0.4",
    "keen-slider": "^6.8.5",
    "react-18-image-lightbox": "^5.1.4",
    "react-blurhash": "^0.3.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.0",
    "react-hotkeys": "^2.0.0",
    "react-modal": "^3.16.1",
    "react-zoom-pan-pinch": "^3.0.6",
    "wouter": "^2.10.0",
    "yet-another-react-lightbox": "^3.3.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --cache --fix -c eslint.config.js --ignore-pattern * --ignore-pattern \"!src/**\"",
    "*.{js,ts,tsx,jsx,html,css,scss}": "prettier --write \"src/**/*\""
  }
}
